    "@voltagent/cli": "^0.1.10",
    "@voltagent/core": "^1.2.10",
    "@voltagent/libsql": "^1.0.0",
    "@voltagent/logger": "^1.0.0",
    "@voltagent/server-hono": "^1.2.5",
    "@voltagent/voice": "^1.0.2",
    "ai": "^5.0.104",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.5",
    "tsdown": "^0.15.2",
