{
  "name": "chatbot-backend",
  "version": "0.1.0",
  "description": "A VoltAgent application",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env ./src",
    "build": "tsdown",
    "start": "node dist/index.js",
    "lint": "biome check ./src",
